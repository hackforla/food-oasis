{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true,
    "commonjs": true,
    "amd": true
  },
  "extends": ["eslint:recommended", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier", "jest"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "@typescript-eslint/no-explicit-any": ["off"]
  }
}
