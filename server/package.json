{
  "name": "foodoasis-web-api",
  "version": "1.0.66",
  "author": "Hack for LA",
  "description": "Web API Server for Food Oasis",
  "main": "server.js",
  "scripts": {
    "test": "mocha",
    "start": "nodemon",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "format": "prettier --write  \"**/*.{js,jsx}\"",
    "lint": "eslint  \"**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix  \"**/*.{js,jsx}\"",
    "migrate": "node-pg-migrate -f db/config.js",
    "loadtest:local": "loadtest -t 60 -c 20 --rps 100 \"http://localhost:5000/api/stakeholderbests?name=&categoryIds[]=1&categoryIds[]=9&latitude=34.0544&longitude=-118.2439&distance=5&isInactive=false&verificationStatusId=0&tenantId=1\" ",
    "loadtest:dev": "loadtest -n 10000 -c 20 \"http://devla.foodoasis.net/api/stakeholderbests?name=&categoryIds[]=1&categoryIds[]=9&latitude=34.0544&longitude=-118.2439&distance=5&isInactive=false&verificationStatusId=0&tenantId=1\" ",
    "loadtest:prod": "loadtest -n 10000 -c 20  \"http://la.foodoasis.net/api/stakeholderbests?name=&categoryIds[]=1&categoryIds[]=9&latitude=34.0544&longitude=-118.2439&distance=5&isInactive=false&verificationStatusId=0&tenantId=1\" ",
    "loadtesttrivial:prod": "loadtest -t 60 -c 20 --rps 100 \"http://la.foodoasis.net/api/categories\" ",
    "loadtesttrivial:dev": "loadtest -t 60 -c 20 --rps 100 \"http://devla.foodoasis.net.herokuapp.com/api/categories\" "
  },
  "lint-staged": {
    "*.{js,md}": "prettier --write",
    "*.js": "eslint --cache --fix"
  },
  "license": "GPL-2.0",
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "camelcase-keys": "^7.0.0",
    "cheerio": "^1.0.0-rc.5",
    "cookie-parser": "^1.4.5",
    "csv-parse": "^4.15.1",
    "csv-stringify": "^5.6.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "json-2-csv": "^3.10.2",
    "jsonwebtoken": "^8.5.1",
    "massive": "^6.8.0",
    "mocha": "^9.1.1",
    "moment": "2.29.2",
    "multer": "^1.4.2",
    "node-pg-migrate": "^5.9.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pino-noir": "^2.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^6.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.4.1"
  }
}
